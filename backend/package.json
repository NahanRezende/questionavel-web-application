{
  "name": "clean-api",
  "version": "3.0.0",
  "description": "An API designed with clean architecture",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev src/main/server.ts",
    "build": "rimraf dist && tsc",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker compose up -d",
    "lint": "eslint . --ext=.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate src/infra/db/typeorm/migrations/base -d src/infra/db/typeorm/ormconfig.ts",
    "migration:run": "yarn typeorm migration:run -d src/infra/db/typeorm/ormconfig.ts",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.ts",
    "test:integration": "npm test -- --watch -c jest-integration-config.ts",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "author": "Davi Banfi Nogueira",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.10.3",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "apollo-server-integration-testing": "^3.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.25.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "graphql": "^15.7.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.3",
    "nodemon": "^2.0.14",
    "pg": "^8.7.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.6",
    "validator": "^13.6.0"
  }
}
